{
  "name": "dr-me-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json,.vue .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.json,.vue . --fix",
    "schema-gen": "node schemagen.js",
    "code-gen": "npx graphql-codegen --config codegen.js",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build"
  },
  "dependencies": {
    "@apollo/client": "3.7.9",
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "2.5.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "apollo-upload-client": "17.0.0",
    "axios": "1.3.4",
    "dotenv": "16.0.3",
    "framer-motion": "9.0.2",
    "graphql": "16.6.0",
    "i18next": "22.4.10",
    "i18next-browser-languagedetector": "7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.3",
    "react-i18next": "12.1.5",
    "react-icons": "4.7.1",
    "react-router-dom": "6.8.1",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.58.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/named-operations-object": "2.3.1",
    "@graphql-codegen/schema-ast": "3.0.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@types/apollo-upload-client": "17.0.2",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "8.3.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-named-exports-order": "0.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-next": "0.0.0",
    "eslint-plugin-storybook": "0.6.8",
    "husky": "8.0.3",
    "path": "0.12.7",
    "prettier": "2.8.4",
    "typescript": "4.9.3",
    "vite": "4.1.0",
    "vite-plugin-minify": "1.5.2"
  }
}
